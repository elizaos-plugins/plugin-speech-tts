{
    "name": "@elizaos-plugins/service-speech-tts",
    "version": "0.1.8+build.1",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist",
        "scripts",
        "package.json",
        "LICENSE",
        "tsup.config.ts"
    ],
    "dependencies": {
        "@deepgram/sdk": "^3.9.0",
        "@elizaos/core": "../eliza/packages/core",
        "echogarden": "2.0.7",
        "formdata-node": "^6.0.3",
        "nodejs-whisper": "0.1.18",
        "openai": "^4.81.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/node": "22.10.8",
        "tsup": "8.3.5",
        "typescript": "^5.7.3"
    },
    "scripts": {
        "build": "tsup --format esm,cjs --dts",
        "dev": "tsup --format esm,cjs --dts --watch",
        "lint": "eslint --fix  --cache .",
        "postinstall": "node scripts/postinstall.js"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "OPENAI_API_KEY": {
                "type": "string"
            },
            "OPENAI_BASE_URL": {
                "type": "string",
                "default": "https://api.openai.com"
            },
            "ELEVENLABS_XI_API_URL": {
                "type": "string",
                "default": "https://api.elevenlabs.io/v1"
            },
            "ELEVENLABS_XI_API_KEY": {
                "type": "string"
            },
            "ELEVENLABS_MODEL_ID": {
                "type": "string",
                "default": "eleven_monolingual_v1"
            },
            "ELEVENLABS_VOICE_ID": {
                "type": "string"
            },
            "ELEVENLABS_VOICE_STABILITY": {
                "type": "string",
                "default": "0.5"
            },
            "ELEVENLABS_VOICE_SIMILARITY_BOOST": {
                "type": "string",
                "default": "0.75"
            },
            "ELEVENLABS_VOICE_STYLE": {
                "type": "string",
                "default": "0"
            },
            "ELEVENLABS_VOICE_USE_SPEAKER_BOOST": {
                "type": "string",
                "default": "true"
            },
            "ELEVENLABS_OPTIMIZE_STREAMING_LATENCY": {
                "type": "string",
                "default": "0"
            },
            "ELEVENLABS_OUTPUT_FORMAT": {
                "type": "string",
                "default": "pcm_16000"
            },
            "VITS_VOICE": {
                "type": "string"
            },
            "VITS_MODEL": {
                "type": "string"
            }
        }
    }
}
