{
    "name": "@elizaos-plugins/service-speech-tts",
    "version": "0.1.8+build.1",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist",
        "scripts",
        "package.json",
        "LICENSE",
        "tsup.config.ts"
    ],
    "dependencies": {
        "@elizaos/core": "workspace:*",
        "echogarden": "2.0.14",
        "gif-frames": "1.0.1",
        "nodejs-whisper": "0.2.6",
        "zod": "3.24.1",
        "openai": "4.80.0",
        "@cliqz/adblocker-playwright": "1.34.0",
        "@echogarden/espeak-ng-emscripten": "0.3.3",
        "@echogarden/kissfft-wasm": "0.2.0",
        "@echogarden/speex-resampler-wasm": "0.2.1",
        "ffmpeg-static": "5.2.0",
        "fluent-ffmpeg": "2.1.3",
        "node-cache": "5.1.2",
        "playwright": "1.49.1",
        "dotenv": "^16.0.3"
    },
    "devDependencies": {
        "@types/node": "22.10.8",
        "tsup": "8.3.5"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "lint": "eslint --fix  --cache .",
        "postinstall": "node scripts/postinstall.js"
    },
    "peerDependencies": {
        "onnxruntime-node": "1.20.1",
        "whatwg-url": "7.1.0"
    },
    "trustedDependencies": {
        "onnxruntime-node": "1.20.1",
        "sharp": "0.33.5"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "OPENAI_API_KEY": {
                "type": "string"
            },
            "ELEVENLABS_XI_API_KEY": {
                "type": "string"
            },
            "ELEVENLABS_MODEL_ID": {
                "type": "string",
                "default": "eleven_monolingual_v1"
            },
            "ELEVENLABS_VOICE_ID": {
                "type": "string"
            },
            "ELEVENLABS_VOICE_STABILITY": {
                "type": "string",
                "default": "0.5"
            },
            "ELEVENLABS_VOICE_SIMILARITY_BOOST": {
                "type": "string",
                "default": "0.75"
            },
            "ELEVENLABS_VOICE_STYLE": {
                "type": "string",
                "default": "0"
            },
            "ELEVENLABS_VOICE_USE_SPEAKER_BOOST": {
                "type": "string",
                "default": "true"
            },
            "ELEVENLABS_OPTIMIZE_STREAMING_LATENCY": {
                "type": "string",
                "default": "0"
            },
            "ELEVENLABS_OUTPUT_FORMAT": {
                "type": "string",
                "default": "pcm_16000"
            },
            "VITS_VOICE": {
                "type": "string"
            },
            "VITS_MODEL": {
                "type": "string"
            }
        }
    }
}
